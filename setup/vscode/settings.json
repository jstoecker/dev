{
    "editor.minimap.enabled": false,
    "editor.overviewRulerBorder": false,
    "editor.formatOnPaste": false,
    "editor.formatOnType": false,
    "editor.tabSize": 4,
    "editor.renderWhitespace": "all",
    "editor.insertSpaces": true,
    "editor.codeLens": false,
    "editor.cursorBlinking": "phase",
    "editor.dragAndDrop": false,
    "editor.rulers": [ 100 ],
    "editor.smoothScrolling": true,
    "editor.mouseWheelScrollSensitivity": 2,
    "editor.renderLineHighlight": "all",
    "diffEditor.renderSideBySide": false,
    "workbench.colorTheme": "One Dark Pro",
    "workbench.startupEditor": "newUntitledFile",
    "terminal.integrated.shell.windows": "C:\\WINDOWS\\System32\\WindowsPowerShell\\v1.0\\powershell.exe",
    "explorer.openEditors.visible": 0,
    "explorer.confirmDelete": false,
    "powershell.codeFormatting.openBraceOnSameLine": false,
    "powershell.integratedConsole.showOnStartup": false,
    "sublimeTextKeymap.promptV3Features": true,
    "gitlens.advanced.messages": {
        "suppressCommitHasNoPreviousCommitWarning": false,
        "suppressCommitNotFoundWarning": false,
        "suppressFileNotUnderSourceControlWarning": false,
        "suppressGitVersionWarning": false,
        "suppressLineUncommittedWarning": false,
        "suppressNoRepositoryWarning": false,
        "suppressResultsExplorerNotice": true,
        "suppressShowKeyBindingsNotice": true
    },
    "gitlens.historyExplorer.enabled": false,
    "gitlens.gitExplorer.files.layout": "list",
    "gitlens.hovers.currentLine.over": "line",
    "gitlens.blame.highlight.locations": [
        "gutter",
        "line",
        "overview"
    ],
    "gitlens.recentChanges.highlight.locations": [
        "line",
        "overview",
        "gutter"
    ],
    "gitlens.defaultGravatarsStyle": "identicon",
    "gitlens.resultsExplorer.files.layout": "list",
    "gitlens.codeLens.enabled": false,
    "git.confirmSync": false,
    "workbench.statusBar.feedback.visible": false,
    "window.title": "${dirty}${activeEditorShort}${separator}${rootName}${separator}",
    "debug.inlineValues": true,
}